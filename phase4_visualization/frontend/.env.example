# Environment Variables Template
# このファイルをコピーして .env.development または .env.production を作成してください

# Backend API URL (Required)
# FastAPI サーバーのURL
VITE_API_BASE_URL=http://localhost:8000

# API Timeout (Optional, default: 30000ms)
# APIリクエストのタイムアウト時間（ミリ秒）
VITE_API_TIMEOUT=30000

# Debug Mode (Optional, default: false)
# デバッグモードの有効化（開発環境でのみtrue推奨）
VITE_DEBUG_MODE=false

# Feature Flags (Optional)
# 循環依存チェック機能の有効化
VITE_ENABLE_CIRCULAR_DEPENDENCY_CHECK=true

# リファクタリングリスク評価機能の有効化
VITE_ENABLE_REFACTORING_RISK_ASSESSMENT=true

# Graph Visualization Settings (Optional)
# デフォルトのグラフ探索深さ
VITE_DEFAULT_GRAPH_DEPTH=3

# 最大グラフ探索深さ
VITE_MAX_GRAPH_DEPTH=10
